server {
    listen 80;
    server_name demo.getpinry.com;

    access_log /var/log/nginx/pinry.access.log;

    location / {
        proxy_set_header X-Forward-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $http_host;
        proxy_redirect off;
        proxy_pass http://localhost:10000;
    }
}
